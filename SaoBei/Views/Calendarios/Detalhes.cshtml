            <p>@Html.DisplayNameFor(model => model.Fevereiro): @Html.DisplayFor(model => model.Fevereiro)</p>
            <p>@Html.DisplayNameFor(model => model.Marco): @Html.DisplayFor(model => model.Marco)</p>
