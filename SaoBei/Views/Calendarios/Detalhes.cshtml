            <p>@Html.DisplayNameFor(model => model.ValorAnuidade): @Html.DisplayFor(model => model.ValorAnuidade)</p>
            <p>@Html.DisplayNameFor(model => model.DataVencimentoAnuidade): @Html.DisplayFor(model => model.DataVencimentoAnuidade)</p>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
        </div>
    </div>
</div>
